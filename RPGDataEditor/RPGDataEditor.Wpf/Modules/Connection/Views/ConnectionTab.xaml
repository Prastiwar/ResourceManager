<UserControl x:Class="RPGDataEditor.Wpf.Connection.Views.ConnectionTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"
             xmlns:controls="clr-namespace:RPGDataEditor.Wpf.Controls"
             Name="CONNECTION"
             prism:ViewModelLocator.AutoWireViewModel="True">
    <controls:SessionControl x:Name="SessionControl"
                             Margin="32"
                             DataContext="{Binding Session}"
                             TypeChange="SessionControl_TypeChange" />
</UserControl>
