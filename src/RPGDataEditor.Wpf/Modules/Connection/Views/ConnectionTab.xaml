<UserControl x:Class="RPGDataEditor.Wpf.Connection.Views.ConnectionTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:controls="clr-namespace:RPGDataEditor.Wpf.Controls"
             Name="CONNECTION"
             Style="{DynamicResource DefaultControlStyle}">
    <controls:SessionControl x:Name="SessionControl"
                             Margin="32"
                             DataContext="{Binding ConnectionSettings}"
                             TypeChange="SessionControl_TypeChange" />
</UserControl>
